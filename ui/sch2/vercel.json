{
  "version": 2,
  "devCommand": "uvicorn app.main:app --host 0.0.0.0 --port 8200",
  "public": false,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./app/app.py"
    }
  ],
  "env": {
    "DB_USER": "root",
    "DB_HOST": "localhost",
    "DB_NAME": "task1",
    "DB_PASSWORD": "password",
    "DB_PORT": "3306"
  }
}

